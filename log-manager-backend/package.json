{
    "name": "log-manager-backend",
    "version": "1.0.0",
    "description": "Structured log ingestion and querying system backend",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "dev:watch": "ts-node-dev --respawn --transpile-only --watch src src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "format:check": "prettier --check src/**/*.ts",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "build:prod": "npm run clean && npm run typecheck && npm run build"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.5.0",
        "@types/express": "^4.17.17",
        "@types/cors": "^2.8.13",
        "@types/jest": "^29.5.4",
        "typescript": "^5.1.6",
        "ts-node-dev": "^2.0.0",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "prettier": "^3.0.2",
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "log",
        "ingestion",
        "monitoring",
        "express",
        "typescript",
        "rest-api"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/log-ingestion-backend.git"
    }
}